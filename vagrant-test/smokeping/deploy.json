{
  "smokeping": {
    "config": {
      "pathnames": {
        "datadir": "/vagrant/smokeping_rrd"
      },
      "targets": [
        {
          "github": [
            {
              "host": "github.com",
              "menu": "github.com",
              "probe": "Curl",
              "title": "gitbub.com"
            },
            {
              "host": "api.github.com",
              "menu": "api.github.com",
              "probe": "Curl",
              "title": "api.gitbub.com"
            }
          ]
        },
        {
          "social": [
            {
              "host": "twitter.com",
              "menu": "twitter.com",
              "probe": "FPing",
              "title": "twitter"
            },
            {
              "host": "facebook.com",
              "menu": "facebook.com",
              "probe": "FPing",
              "title": "facebook"
            }
          ]
        }
      ]
    }
  }
}
